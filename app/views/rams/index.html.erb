<p id="notice"><%= notice %></p>

<h1>Listing Rams</h1>

<table>
  <thead>
    <tr>
      <th>Workpackage</th>
      <th>Node</th>
      <th>Project</th>
      <th>Level</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rams.each do |ram| %>
      <tr>
        <td><%= ram.workpackage %></td>
        <td><%= ram.node %></td>
        <td><%= ram.project %></td>
        <td><%= ram.level %></td>
        <td><%= ram.order %></td>
        <td><%= link_to 'Show', ram %></td>
        <td><%= link_to 'Edit', edit_ram_path(ram) %></td>
        <td><%= link_to 'Destroy', ram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ram', new_ram_path %>
