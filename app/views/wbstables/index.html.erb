<p id="notice"><%= notice %></p>

<h1>Listing Wbstables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wbstables.each do |wbstable| %>
      <tr>
        <td><%= wbstable.name %></td>
        <td><%= wbstable.project.name %></td>
        <td><%= link_to 'Show', wbstable %></td>
        <td><%= link_to 'Edit', edit_wbstable_path(wbstable) %></td>
        <td><%= link_to 'Destroy', wbstable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wbstable', new_wbstable_path %>
